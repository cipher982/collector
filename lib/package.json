{
  "name": "@collector/context",
  "version": "0.1.0",
  "description": "Lightweight, tree-shakeable library for collecting browser/device context",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./collectors/*": {
      "types": "./dist/collectors/*.d.ts",
      "import": "./dist/collectors/*.js",
      "require": "./dist/collectors/*.cjs"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "import": "./dist/core/*.js",
      "require": "./dist/core/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "analytics",
    "visitor-context",
    "browser-fingerprinting",
    "web-vitals",
    "performance"
  ],
  "author": "David Rose",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "happy-dom": "^20.0.11",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
